      "dest": "/api/server.js"
    },
    {
      "src": "/api/comments",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/admin/usercomments",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/submitscore",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/users/results",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/highestscorer",
      "dest": "/api/server.js"
