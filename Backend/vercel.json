{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/register",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/admin/dashboard",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/admin",
      "dest": "/api/server.js"
    },
    {
