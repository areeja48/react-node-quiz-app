{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/register",
      "dest": "/api/server.js"
    },
    {
      "src": "/auth/login",
      "dest": "/api/server.js"
    },
    {
      "src": "/auth/admin/dashboard",
      "dest": "/api/server.js"
    },
    {
      "src": "/auth/admin",
      "dest": "/api/server.js"
    },
    {
      "src": "/auth/logout",
      "dest": "/api/server.js"
    },
    {
      "src": "/auth/generateOtp",
      "dest": "/api/server.js"
    },
    {
      "src": "/auth/reset",
      "dest": "/api/server.js"
    },
    {
      "src": "/auth/comments",
      "dest": "/api/server.js"
    },
    {
      "src": "/auth/admin/usercomments",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/submitscore",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/users/results",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/highestscorer",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/questions",
      "dest": "/api/server.js"
    }
  ]
}

