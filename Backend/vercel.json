{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/uploads/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/register",
      "dest": "/api/server.js"
    },
    {
      "src": "/login",
      "dest": "/api/server.js"
    },
    {
      "src": "/admin/dashboard",
      "dest": "/api/server.js"
    },
    {
      "src": "/admin",
      "dest": "/api/server.js"
    },
    {
      "src": "/logout",
      "dest": "/api/server.js"
    },
    {
      "src": "/generateOtp",
      "dest": "/api/server.js"
    },
    {
      "src": "/reset",
      "dest": "/api/server.js"
    },
    {
      "src": "/comments",
      "dest": "/api/server.js"
    },
    {
      "src": "/admin/usercomments",
      "dest": "/api/server.js"
    },
    {
      "src": "/submitscore",
      "dest": "/api/server.js"
    },
    {
      "src": "/users/results",
      "dest": "/api/server.js"
    },
    {
      "src": "/highestscorer",
      "dest": "/api/server.js"
    },
    {
      "src": "/questions",
      "dest": "/api/server.js"
    }
  ]
}

